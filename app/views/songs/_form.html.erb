<%= simple_form_for(@song) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :lyric %>
    <%= f.input :views %>
    <%= f.input :keywords %>
    <%= f.input :uploader %>
    <%= f.input :performer %>
    <%= f.input :writer %>
    <%= f.input :album %>
    <%= f.input :views_month %>
    <%= f.input :status %>
    <%= f.input :filename %>
    <%= f.association :category %>
    <%= f.association :postcard %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
